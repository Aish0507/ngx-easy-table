{
  "extends": [
    "tslint-sonarts",
    "tslint:recommended",
    "rxjs-tslint-rules",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["node_modules/codelyzer"],
  "rules": {
    "cognitive-complexity": [true, 10],
    "prefer-object-spread": true,
    "no-submodule-imports": false,
    "no-implicit-dependencies": false,
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "typedef": [true, "call-signature", "property-declaration"],
    "interface-name": [true, "never-prefix"],
    "no-small-switch": false,
    "max-classes-per-file": false,
    "prefer-for-of": false,
    "no-namespace": false,
    "no-object-literal-type-assertion": false,
    "no-hardcoded-credentials": true,
    "max-switch-cases": false,
    "bool-param-default": true,
    "prefer-template": false,
    "arrow-return-shorthand": true,
    "callable-types": true,
    "class-name": true,
    "deprecation": {
      "severity": "warn"
    },
    "forin": true,
    "import-blacklist": [true, "rxjs/Rx"],
    "interface-over-type-literal": true,
    "member-access": false,
    "member-ordering": [
      true,
      {
        "order": ["static-field", "instance-field", "static-method", "instance-method"]
      }
    ],
    "no-arg": true,
    "no-bitwise": true,
    "no-console": [true, "debug", "log", "time", "timeEnd", "trace"],
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-empty": true,
    "no-empty-interface": true,
    "no-eval": true,
    "no-inferrable-types": true,
    "no-misused-new": true,
    "no-non-null-assertion": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-string-throw": true,
    "no-switch-case-fall-through": true,
    "no-unnecessary-initializer": true,
    "no-unused-expression": true,
    "no-var-keyword": true,
    "object-literal-sort-keys": false,
    "prefer-const": true,
    "radix": true,
    "triple-equals": [true, "allow-null-check"],
    "unified-signatures": true,
    "variable-name": [true, "allow-leading-underscore"],
    "directive-selector": [false, "attribute", "app", "camelCase"],
    "component-selector": [false, "element", "app", "kebab-case"],
    "no-output-on-prefix": true,
    "no-input-rename": true,
    "no-output-rename": true,
    "use-lifecycle-interface": true,
    "use-pipe-transform-interface": true,
    "component-class-suffix": true,
    "directive-class-suffix": true,
    "trailing-comma": false,
    "quotemark": [true, "single"],
    "arrow-parens": false,
    "max-line-length": [false, 128],
    "parameters-max-number": [true, 5],
    "object-literal-key-quotes": [true, "as-needed"],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ],
    "no-misleading-array-reverse": false,
    "no-useless-cast": true,
    "no-variable-usage-before-declaration": true,
    "no-empty-nested-blocks": true,
    "no-big-function": [true, 35],
    "function-constructor": true,
    "ordered-imports": false,
    "no-duplicate-string": false,
    "no-same-line-conditional": true,
    "ban": [true, "eval", "alert", "object[]"],
    "rxjs-ban-operators": {
      "options": [
        {
          "pluck": "`pluck('foo', 'bar')` is `map(x => x?.foo?.bar)`"
        }
      ],
      "severity": "error"
    },
    "no-collapsible-if": true,
    "no-dead-store": true,
    "no-duplicate-in-composite": true,
    "no-nested-switch": true,
    "no-identical-functions": true,
    "no-redundant-parentheses": true,
    "no-redundant-jump": true,
    "no-useless-catch": true,
    "no-all-duplicated-branches": true,
    "no-case-with-or": true,
    "no-collection-size-mischeck": true,
    "no-element-overwrite": true,
    "no-empty-array": true,
    "no-empty-destructuring": true,
    "no-identical-conditions": true,
    "no-identical-expressions": true,
    "no-ignored-initial-value": true,
    "no-in-misuse": true,
    "no-misspelled-operator": true,
    "no-self-assignment": true,
    "no-try-promise": true,
    "no-unthrown-error": true,
    "no-use-of-empty-return-value": true,
    "no-useless-increment": true,
    "no-useless-intersection": true,
    "static-this": true,
    "unnecessary-else": true,
    "one-variable-per-declaration": true,
    "no-commented-code": true,
    "no-redundant-jsdoc": true,
    "template-banana-in-box": true,
    "no-lifecycle-call": true,
    "no-conflicting-lifecycle": true,
    "use-component-selector": true,
    "no-queries-metadata-property": true,
    "no-array-delete": true,
    "template-conditional-complexity": [true, 8],
    "template-no-negated-async": true,
    "array-type": false,
    "no-pipe-impure": true,
    "unnecessary-bind": true,
    "no-inputs-metadata-property": true,
    "no-outputs-metadata-property": true,
    "no-host-metadata-property": true,
    "use-component-view-encapsulation": false,
    "no-attribute-decorator": true,
    "no-unused-css": false,
    "contextual-lifecycle": true,
    "template-use-track-by-function": false,
    "template-i18n": false,
    "no-for-in-array": true,
    "no-dynamic-delete": true,
    "no-tautology-expression": true,
    "ban-comma-operator": true,
    "curly": true,
    "no-angle-bracket-type-assertion": true,
    "no-trailing-whitespace": true,
    "one-line": true,
    "import-spacing": true,
    "indent": [true, "spaces"],
    "use-isnan": true,
    "prefer-output-readonly": true,
    "mccabe-complexity": [true, 20],
    "no-forward-ref": true,
    "prefer-inline-decorator": false,
    "no-unused-array": true,
    "no-unnecessary-class": [
      true,
      "allow-empty-class",
      "allow-static-only",
      "allow-constructor-only"
    ],
    "align": [true, "parameters", "statements"],
    "no-output-native": true,
    "no-return-type-any": true,
    "max-file-line-count": [true, 560],
    "template-cyclomatic-complexity": [true, 60],
    "rxjs-prefer-angular-takeuntil": {
      "severity": "error"
    },
    "rxjs-no-unsafe-takeuntil": {
      "severity": "error"
    },
    "ban-ts-ignore": true,
    "unnecessary-constructor": true,
    "no-alphabetical-sort": false,
    "max-union-size": false
  }
}
